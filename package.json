    "bootstrap": "^3.3.7",
    "bootstrap-material-design": "^0.5.10",
    "chart.js": "^2.4.0",
    "codelyzer": "~2.0.0-beta.1",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
